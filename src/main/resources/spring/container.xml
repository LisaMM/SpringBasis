<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns='http://www.springframework.org/schema/beans'
	xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
	xsi:schemaLocation='http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd'>
	<bean id='classA' class='be.vdab.ClassA'>
		<property name='telefoonNrHelpdesk' value='01/234.567.8' />
		<property name='aantalPogingenUpdateKlant' value='3' />
		<property name='maxTemperatuurMotor' value='200.2' />
		<property name='urlKoersenVreemdeMuntenTOVEuro'
			value='http://www.ecb.europa.eu/stats/eurofxref/eurofxref-daily.xml' />
		<property name='defaultTaalEnLandVanDeApplicatie' value='nl_BE' />
		<property name='bestandMetTeImporterenData' value='c:/data/postcodes.txt' />
		<property name='standaardBladRichtingInPrinter' value='LIGGEND' />
	</bean>
	<bean id='classB' class='be.vdab.ClassB'>
		<constructor-arg value='01/234.567.8' />
		<constructor-arg value='3' />
	</bean>
	<bean id='persoonViewer' class='be.vdab.PersoonViewer'>
		<constructor-arg>
			<list>
				<value>VOORNAAM</value>
				<value>FAMILIENAAM</value>
				<value>AANTAL_KINDEREN</value>
			</list>
		</constructor-arg>
	</bean>
	<bean id='helpdeskMedewerkers' class='be.vdab.HelpdeskMedewerkers'>
		<constructor-arg>
			<props>
				<prop key='Joe'>123</prop>
				<prop key='Jack'>456</prop>
				<prop key='William'>789</prop>
				<prop key='Averell'>012</prop>
			</props>
		</constructor-arg>
	</bean>
	<bean id='teller1' class='be.vdab.Teller' scope='prototype'>
		<constructor-arg value='7' />
	</bean>
	<bean id='teller2' class='be.vdab.Teller'>
		<constructor-arg value='666' />
	</bean>
	<bean id='namenDAOTXT' class='be.vdab.dao.NamenDAOTXT'>
		<property name='bestand' value='c:/data/namen.txt' />
	</bean>
	<bean id='namenDAOXML' class='be.vdab.dao.NamenDAOXML'>
		<property name='bestand' value='c:/data/namen.xml' />
	</bean>
	<bean id='namenService' class='be.vdab.services.NamenServiceImpl'>
		<constructor-arg ref='namenDAOXML' />
	</bean>
</beans>